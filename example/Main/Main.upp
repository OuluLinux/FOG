description "The main FOG project.\377";

uses
	Library;

include
	.;

file
	Main.mfog highlight cpp,
	ExampleApp.fog highlight cpp,
	"FOG Generated" readonly separator,
	Main.hxx,
	Main.cxx,
	ExampleApp.hxx,
	ExampleApp.cxx,
	Library.hxx,
